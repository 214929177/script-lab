    "enzyme-to-json": "^3.3.4",
    "fs-extra": "^6.0.1",
    "jest": "^22.4.3",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.3.2",
    "redux-devtools": "^3.4.1",
    "ts-jest": "^22.4.6",
    "typescript": "^3.0.1"
